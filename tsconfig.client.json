{
  "include": [
    "common",
    "client",
  ],
  "compilerOptions": {
    "module": "es6",
    "sourceMap": true,
    "target": "es5",
    "moduleResolution": "node",
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "strictNullChecks": true,
    "noImplicitAny": true
  }
}